
fn dummy() *int {
  return null as *int
}

fn none_dummy() *char {
  return "some static text"
}

fn main() int
{
  v := [3, 1, 2]
  printf("%d %d %d\n", v[0], v[1], v[2])

  if (x := dummy())
    printf("%d\n", *x)
  else
    printf("%p\n", x)

  if (x := none_dummy())
    printf("%s\n", x)
  else
    printf("nooo: %p\n", x)

  return 0
}
