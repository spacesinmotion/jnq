

fn dynamic_array_null_test() {
  print("..dynamic_array_null_test\n")

  dyn_null := null as[*]i32

  ASSERT(dyn_null == null)
  ASSERT(null == dyn_null)
  ASSERT(!(dyn_null != null))
  ASSERT(!(null != dyn_null))

  ASSERT(!dyn_null)

  dyn_null = new[]i32{}
  ASSERT(dyn_null)
  delete dyn_null
}

fn main() int {
  print("any_type\n")

  dynamic_array_null_test()

  print(" ...ok\n")
  return 0
}
