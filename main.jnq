
fn dummy(a int) int {
  return a
}

type TestE enum {
  A = 3,
  B,
  C
}

fn main(argc i32, argv[]*char) int
{
  for (i := 0; i < argc; ++i)
    printf("%s ", argv[i])
  printf("\n", v)

  v := [3, 1, 2]
  printf("%d %d %d\n", v[0], v[1], v[2])

  switch (val := dummy(3)) {
  case 3: printf("Yes %d\n", val)
  default: ASSERT(false)
  }

  ASSERT(TestE.A as i32 == 3)
  ASSERT(TestE.B as i32 == 4)
  ASSERT(TestE.C as i32 == 5)

  printf("ok\n")
  return 0
}
