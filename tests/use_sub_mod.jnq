
use sub.util

fn main() int {
  printf("use sub module\n")

  ASSERT(42 == util.add(31, 11))

  p util.Point = util.Point{9, 7}
  ASSERT(p.x == 9)
  ASSERT(p.y == 7)

  p.x = 4
  p.y = 5
  ASSERT(p.x == 4)
  ASSERT(p.y == 5)

  p = p.neg()
  ASSERT(p.x == -4)
  ASSERT(p.y == -5)

  printf(" ...ok\n")
}