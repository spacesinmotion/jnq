
fn dummy(a int) int {
  return a
}

fn main() int {
  printf("switches\n")
  a := 0

  switch (3) {
  case 1:
    ASSERT(false)
  case 3:
    a = 42
  case 5:
    ASSERT(false)
  default:
    ASSERT(false)
  }

  ASSERT(a == 42)

  switch (9898) {
  case 1:
    ASSERT(false)
  case 3:
    ASSERT(false)
  case 5:
    ASSERT(false)
  default:
    a = 63
  }
  ASSERT(a == 63)

  val := 42
  switch (val := dummy(3)) {
  case 3:
    ASSERT(val == 3)
  default:
    ASSERT(false)
  }
  ASSERT(val == 42)

  printf(" ...ok\n")
  return 0
}