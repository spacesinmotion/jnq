
use sub.util

type Vec2 struct {
  x float, 
  y float,
}

type Vec3 struct {
  x float 
  y float
  z float 
}

type VecU uniontype { Vec3, Vec2, util.Point, util.Nix }

fn main() int {
  printf("uniontypes\n")

  x VecU = VecU{Vec2{0.1f, 0.3f}}
  y VecU = VecU{Vec3{0.1f, 0.3f}}
  p VecU = VecU{util.Point{20, 42}}
  n VecU = VecU{util.Nix.Nice}

  printf(" ...ok\n")
}